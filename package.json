{
  "name": "resicalc",
  "author": {
    "name": "Waseeen",
    "email": "aaaa@waseeen.ru",
    "url": "https://waseeen.ru"
  },
  "repository": "github:waseeen/resicalc",
  "homepage": "https://resicalc.waseeen.ru",
  "keywords": [
    "resistor",
    "calculator",
    "typescript",
    "vite",
    "react"
  ],
  "description": "Resistor tolerance, temperature coefficient and resistance calculator",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host",
    "gpa": "pwa-assets-generator --preset minimal public/favicon.svg",
    "build:pwa": "tsc && vite build && npm run gpa",
    "bnr": "npm run build:pwa && npm run preview"
  },
  "dependencies": {
    "i18next": "^23.10.1",
    "i18next-http-backend": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "vite": "^5.1.6",
    "vite-plugin-pwa": "^0.19.2",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2"
  }
}
